Depends: libmagic1, media-types | mime-support, php8.2-cli, php8.2-common (= 8.2.7-1ubuntu1), php8.2-opcache, tzdata, ucf, libargon2-1 (>= 0~20171227), libc6 (>= 2.35), libpcre2-8-0 (>= 10.32), libsodium23 (>= 1.0.18), libssl3 (>= 3.0.0), libxml2 (>= 2.8.0), zlib1g (>= 1:1.1.4)
